{
    "presets": [
        [
            "@babel/preset-env",
            {
                "useBuiltIns": "entry",
                "corejs": "3.22"
            }
        ],

        "@babel/preset-flow"
    ],
    "plugins": [
        "@babel/plugin-proposal-export-default-from",
        [
            "module-resolver",
            {
                "root": ["./src"],
                "alias": {
                    "utilities": "./src/utilities"
                }
            }
        ]
    ]
}